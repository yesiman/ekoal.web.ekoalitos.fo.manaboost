          img(id="ie"+"item.objectid",style="left:100px;position: absolute;z-index: 1;",src=BOWL_MEDIA_URL+"/node/thumbnails/"+bh._source.external.origin+"/"+bh._source.external.thumbnail, alt='Project Name')
          h6.title(style="display:inline;")
            a(href=pathDoc,target="_blank")= title
          span.meta(style="display:block;margin-top:10px;")
            span.btn-info.badge
              = bh.custom.origin
            span.btn-primary.badge
              = (bh._source.meta.author?bh._source.meta.author:"Inconnu")
            span.btn-default.badge
              = (bh._source.meta.language?bh._source.meta.language:"Inconnu")
            span.btn-success.badge
              = (bh._source.file.extension?bh._source.file.extension:"Inconnu")
            if bh._source.external.keywords
              span.btn-warning.badge
                each k in bh._source.external.keywords
                  = k
          //span.meta(style="display:block;margin-top:10px;")
            span.meta-part
              a(href="javascript:void(0)")
                i.ico-users
                span= (bh._source.meta.author?bh._source.meta.author:"Inconnu")
            span.meta-part
              a(href="javascript:void(0)")
                i.ico-flag
                span= (bh._source.meta.language?bh._source.meta.language:"Inconnu")
            if bh._source.external.keywords
              span.meta-part
                a(href="javascript:void(0)")
                  i.ico-key
                  span
                    each k in bh._source.external.keywords
                      = k
            span.meta-part
              a(href="javascript:void(0)")
                i.ico-file
                span= (bh._source.file.extension?bh._source.file.extension:"Inconnu")
            span.meta-part
              a(href="javascript:void(0)")
                i.ico-note
                //- iint = document.getElementById(bh._source.external.origin)
                span= bh.custom.origin
          p.v-card__text!= (bh.highlight?bh.highlight.content:"")

          //a.btn_a(href='javascript:void(0)' onclick="VV_dir_global.gpt.openChat(this,'"+bh._id+"')")
            span
              i.in_left.ico-angle-right
              span=" "
              img(src="../assets/images/ChatGPT.png", alt='GPT', height="30")
              i.in_right.ico-angle-right
          a.btn_a(href='javascript:void(0)' onclick="VV_dir_global.bowl.addCart('"+bh._id+"')")
            span
              i.in_left.ico-angle-right
              span= "Panier"
              i.in_right.ico-angle-right
